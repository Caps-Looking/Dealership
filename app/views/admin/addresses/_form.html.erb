<%= f.simple_fields_for :address do |a| %>
    <%= a.input :cep, input_html: { autocomplete: "off", onkeypress: "$(this).mask('00000-000')" }%>
    <%= a.input :state, input_html: { autocomplete: "off" } %>
    <%= a.input :city, input_html: { autocomplete: "off" } %>
    <%= a.input :district, input_html: { autocomplete: "off" } %>
    <%= a.input :street, input_html: { autocomplete: "off" } %>
    <%= a.input :number, input_html: { autocomplete: "off" } %>
    <%= a.input :complement, input_html: { autocomplete: "off" } %>
<% end %>
