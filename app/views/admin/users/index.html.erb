<h1><%= t('views.users')%></h1>
<%= link_to t('views.user.new'), new_admin_user_path %>
<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.id %>
      <a href="<%= admin_user_path(user) %>">
        <%= user.name %>
      </a>
      <%= user.username %>
      <%= user.user_type_humanize %>
    </li>
  <% end %>
</ul>
